{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
      "@entities/*": [
        "./src/constants/common/entities/*"
      ],
      "@shared/*": [
        "./src/constants/common/shared/*"
      ],
      "@process/*": [
        "./src/constants/feature/process/*"
      ],
      "@screens/*": [
        "./src/constants/feature/screens/*"
      ],
      "@widget/*": [
        "./src/constants/feature/widget/*"
      ],
      "@context/*": [
        "./src/context/*"
      ],
      "@api/*": [
        "./src/api/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@bridges/*": [
        "./src/bridges/*"
      ],
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}